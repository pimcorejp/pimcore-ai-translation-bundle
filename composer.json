{
    "name": "pimcorejp/pimcore-ai-translation-bundle",
    "type": "pimcore-bundle",
    "license": "proprietary",
    "description": "AI-powered translation bundle for PIMCORE with DeepL and Gemini support. Preserves HTML structure in WYSIWYG fields.",
    "keywords": ["pimcore", "translation", "deepl", "gemini", "ai", "wysiwyg"],
    "homepage": "https://github.com/pimcorejp/pimcore-ai-translation-bundle",
    "authors": [
        {
            "name": "PIMCORE JP Community",
            "email": "support@pimcorejp.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "pimcore/pimcore": "^10.5 || ^11.0 || ^12.0",
        "symfony/http-client": "^5.4 || ^6.0 || ^7.0",
        "symfony/cache": "^5.4 || ^6.0 || ^7.0"
    },
    "autoload": {
        "psr-4": {
            "pimcorejp\\TranslationBundle\\": "src/"
        }
    },
    "extra": {
        "pimcore": {
            "bundles": [
                "pimcorejp\\TranslationBundle\\TranslationBundle"
            ]
        }
    }
}